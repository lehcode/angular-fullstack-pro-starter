{
  "name": "vindb-ui",
  "version": "0.0.1",
  "description": "VIN Frontend Application",
  "author": "Antony Repin",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "ng": "ng",
    "build": "ng run ui:build:production",
    "build-ssr": "ng build --prod && ng run ui:server:production",
    "ci": "yarn lint-all && yarn test && yarn e2e",
    "e2e": "ng e2e",
    "e2e-ci": "ng e2e:ci",
    "doc": "npx @compodoc/compodoc -p tsconfig.json -s",
    "i18n-extract": "ng extract-i18n --output-path src/locale",
    "lint-added-html": "git status -s | egrep \"^[AM]  \"  | egrep \\.html$ | cut -d ' ' -f 3 | xargs npx eslint",
    "lint-added-scss": "git status -s | egrep \"^[AM]  \"  | egrep \\.scss | cut -d ' ' -f 3 | xargs npx stylelint --syntax scss --allow-empty-input --stdin",
    "lint-added-ts": "git status -s | egrep \"^[AM]  \"  | egrep \\.ts$ | cut -d ' ' -f 3 | xargs npx eslint",
    "lint-ci": "eslint 'src/**/*.{ts,tsx,html}' --quiet && ng run ui:lint-scss",
    "lint-all": "yarn lint-ts && yarn lint-js && yarn lint-html && yarn lint-scss",
    "lint-html": "eslint 'src/**/*.html'",
    "lint-js": "eslint '*.js'",
    "lint-scss": "npx stylelint --syntax scss src/**/*.scss",
    "lint-ts": "eslint 'src/**/*.{ts,tsx}'",
    "prerender": "ng run ui:prerender",
    "prettier-format": "prettier --config .prettierrc 'src/**/*.(ts|html|scss)' --write",
    "start": "ng serve --host 0.0.0.0 --publicHost=carvin.local --port 4300 --watch",
    "start-i18n-en": "yarn start -- --configuration en",
    "start-i18n-ru": "yarn start -- --configuration ru",
    "start-i18n-uk": "yarn start -- --configuration uk",
    "start-ssr": "ng run ui:serve-ssr",
    "test": "ng test ui --progress --watch=false",
    "test-watch": "ng test ui --reporters dots,kjhtml --browsers Chrome --watch"
  },
  "dependencies": {
    "@angular/animations": "~11.0.5",
    "@angular/cdk": "^11.0.3",
    "@angular/common": "~11.0.5",
    "@angular/compiler": "~11.0.5",
    "@angular/core": "~11.0.5",
    "@angular/flex-layout": "^11.0.0-beta.33",
    "@angular/forms": "~11.0.5",
    "@angular/material": "^11.0.3",
    "@angular/platform-browser": "~11.0.5",
    "@angular/platform-browser-dynamic": "~11.0.5",
    "@angular/platform-server": "^11.0.5",
    "@angular/router": "~11.0.5",
    "@nguniversal/express-engine": "^11.0.1",
    "express": "^4.15.2",
    "ngx-logger": "^4.1.9",
    "rxjs": "~6.6.0",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1100.5",
    "@angular-eslint/builder": "0.8.0-beta.6",
    "@angular-eslint/eslint-plugin": "0.8.0-beta.6",
    "@angular-eslint/eslint-plugin-template": "0.8.0-beta.6",
    "@angular-eslint/schematics": "^0.8.0-beta.6",
    "@angular-eslint/template-parser": "0.8.0-beta.6",
    "@angular/cli": "~11.0.5",
    "@angular/compiler-cli": "^11.0.5",
    "@angular/localize": "^11.0.5",
    "@compodoc/compodoc": "^1.1.11",
    "@nguniversal/builders": "^11.0.1",
    "@types/express": "^4.17.9",
    "@types/jasmine": "^3.6.2",
    "@types/node": "^12.11.1",
    "@typescript-eslint/eslint-plugin": "4.3.0",
    "@typescript-eslint/parser": "4.3.0",
    "eslint": "^7.6.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-html": "^6.1.1",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jsdoc": "30.7.6",
    "eslint-plugin-prefer-arrow": "1.2.2",
    "eslint-plugin-prettier": "^3.3.0",
    "husky": "^4.3.6",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.1.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "^2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "prettier": "^2.2.1",
    "protractor": "^7.0.0",
    "puppeteer": "^5.5.0",
    "stylelint": "^13.8.0",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-sass-guidelines": "^7.1.0",
    "stylelint-prettier": "^1.1.2",
    "stylelint-scss": "^3.18.0",
    "ts-node": "~8.3.0",
    "typescript": "~4.0.2"
  }
}
