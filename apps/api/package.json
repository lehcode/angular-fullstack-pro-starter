{
  "name": "@aims/api",
  "version": "0.0.1",
  "description": "Starter NestJS application",
  "author": "Lehcode<53556648+lehcode@users.noreply.github.com>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "yarn prebuild && nest build",
    "ci": "yarn lint && yarn test && yarn build",
    "doc": "npx @compodoc/compodoc -p tsconfig.json -s",
    "format": "prettier --write src/**/*.ts test/**/*.ts",
    "lint": "eslint {src,apps,libs,test}/**/*.ts --fix",
    "sls-deploy": "rimraf .build && SLS_DEBUG=* sls deploy",
    "sls-local": "rimraf .build && SLS_DEBUG=* sls offline start --noPrependStageInUrl --httpPort 3300",
    "start": "nest start --debug 0.0.0.0:9229 --watch",
    "start-prod": "node dist/main",
    "test": "jest",
    "test-cov": "jest --coverage",
    "test-debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand"
  },
  "dependencies": {
    "@nestjs/axios": "^3.0.2",
    "@nestjs/common": "^10.3.3",
    "@nestjs/config": "^3.2.0",
    "@nestjs/core": "^10.3.3",
    "@nestjs/mongoose": "^10.0.4",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^10.3.3",
    "@nestjs/typeorm": "^10.0.2",
    "accept-language-parser": "^1.5.0",
    "axios": "^1.6.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "html-to-markdown": "^1.0.0",
    "i18next": "^23.8.2",
    "i18next-chained-backend": "^4.6.2",
    "i18next-fs-backend": "^2.3.1",
    "i18next-http-backend": "^2.4.3",
    "i18next-http-middleware": "^3.5.0",
    "i18next-json-sync": "^3.1.2",
    "i18next-node-fs-backend": "^2.1.3",
    "i18next-node-mongo-backend": "^0.0.4",
    "joi": "^17.12.1",
    "lodash": "^4.17.21",
    "moment": "^2.30.1",
    "mongoose": "^7.3.3",
    "mysql2": "^3.9.1",
    "passport": "^0.7.0",
    "passport-local": "^1.0.0",
    "reflect-metadata": "^0.2.1",
    "rimraf": "^5.0.5",
    "showdown": "^2.1.0",
    "typeorm": "^0.3.20"
  },
  "devDependencies": {
    "@compodoc/compodoc": "^1.1.23",
    "@nestjs/cli": "^10.3.2",
    "@nestjs/schematics": "^10.1.0",
    "@nestjs/testing": "^10.3.3",
    "@tsconfig/recommended": "^1.0.3",
    "@types/accept-language-parser": "^1.5.6",
    "@types/aws-lambda": "^8.10.133",
    "@types/express": "^4.17.21",
    "@types/i18next-fs-backend": "^1.1.5",
    "@types/i18next-node-fs-backend": "^2.1.5",
    "@types/jest": "29.5.12",
    "@types/lodash": "^4.14.202",
    "@types/node": "20.11.17",
    "@types/passport": "^1.0.16",
    "@types/showdown": "^2.0.6",
    "@types/source-map-support": "^0.5.10",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^7.0.1",
    "@typescript-eslint/parser": "^7.0.1",
    "dotenv": "^16.4.4",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "29.7.0",
    "prettier": "^3.2.5",
    "reflect-metadata": "^0.2.1",
    "serverless-offline": "^13.3.3",
    "serverless-plugin-typescript": "^2.1.5",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.4",
    "ts-jest": "^29.1.2",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.3.3",
    "webpack": "^5.90.1"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "./src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "moduleNameMapper": {
      "@base/(.*)": "<rootDir>/$1",
      "@admin/(.*)": "<rootDir>/modules/admin/$1",
      "@api/(.*)": "<rootDir>/modules/api/$1",
      "@db/(.*)": "<rootDir>/modules/db/$1",
      "@services/(.*)": "<rootDir>/services/$1"
    }
  }
}
